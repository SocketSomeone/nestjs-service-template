{
	"$schema": "https://swc.rs/schema.json",
	"sourceMaps": true,
	"jsc": {
		"parser": {
			"syntax": "typescript",
			"decorators": true
		},
		"target": "es2022",
		"baseUrl": "./",
		"paths": {
			"@src/*": [
				"./src/*"
			],
			"@common/*": [
				"./src/common/*"
			],
			"@shared/*": [
				"./src/shared/*"
			]
		}
	},
	"minify": false,
	"module": {
		"strict": true,
		"type": "commonjs"
	}
}
