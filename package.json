{
	"name": "nestjs-service-template",
	"description": "ðŸ“œ My template for creating NestJS Services",
	"version": "1.0.0",
	"scripts": {
		"prebuild": "rimraf dist",
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\"",
		"start": "nest start",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main",
		"lint": "eslint --ignore-path .gitignore \"{src,apps,libs,test}/**/*.ts\"",
		"lint:fix": "npm run lint -- --fix",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"prepare": "husky"
	},
	"lint-staged": {
		"*.ts": [
			"npm run format",
			"npm run lint:fix"
		]
	},
	"directories": {
		"test": "test"
	},
	"main": "./dist/main.js",
	"types": "./dist/main.d.ts",
	"keywords": [],
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/SocketSomeone/nestjs-open-source-template"
	},
	"bugs": {
		"url": "https://github.com/SocketSomeone/nestjs-open-source-template"
	},
	"author": "Alexey Filippov <socket.someone@gmail.com>",
	"contributors": [
		"Alexey Filippov <socket.someone@gmail.com>"
	],
	"dependencies": {
		"@nestjs/axios": "^3.1.2",
		"@nestjs/common": "10.4.11",
		"@nestjs/config": "^3.3.0",
		"@nestjs/core": "10.4.11",
		"@nestjs/jwt": "^10.2.0",
		"@nestjs/platform-express": "^10.4.11",
		"@nestjs/swagger": "^8.0.7",
		"axios": "^1.7.8",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.1",
		"reflect-metadata": "^0.2.2",
		"rxjs": "^7.8.1"
	},
	"devDependencies": {
		"@commitlint/cli": "19.6.0",
		"@commitlint/config-angular": "19.6.0",
		"@nestjs/cli": "^10.4.8",
		"@nestjs/schematics": "^10.2.3",
		"@nestjs/testing": "^10.4.12",
		"@swc/cli": "^0.5.1",
		"@swc/core": "^1.9.3",
		"@swc/jest": "^0.2.37",
		"@types/jest": "29.5.14",
		"@types/node": "22.10.1",
		"@typescript-eslint/eslint-plugin": "5.62.0",
		"@typescript-eslint/parser": "5.62.0",
		"eslint": "8.57.1",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-prettier": "5.2.1",
		"husky": "9.1.7",
		"jest": "29.7.0",
		"lint-staged": "15.2.10",
		"prettier": "3.4.1",
		"release-it": "17.10.0",
		"rimraf": "6.0.1",
		"ts-jest": "29.2.5",
		"ts-loader": "^9.5.1",
		"ts-node": "10.9.2",
		"tsconfig-paths": "^4.2.0",
		"typescript": "5.7.2"
	}
}
