{
	"name": "nestjs-service-template",
	"description": "ðŸ“œ My template for creating NestJS Services",
	"version": "1.0.0",
	"scripts": {
		"prebuild": "rimraf dist",
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\"",
		"start": "nest start",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main",
		"lint": "eslint --ignore-pattern .gitignore \"{src,apps,libs,test}/**/*.ts\"",
		"lint:fix": "npm run lint -- --fix",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:ci": "jest --ci --passWithNoTests --coverage",
		"prepare": "husky",
		"publish:git": "release-it"
	},
	"lint-staged": {
		"*.ts": [
			"npm run format",
			"npm run lint:fix"
		]
	},
	"directories": {
		"test": "test"
	},
	"main": "./dist/main.js",
	"types": "./dist/main.d.ts",
	"keywords": [],
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/SocketSomeone/nestjs-service-template"
	},
	"bugs": {
		"url": "https://github.com/SocketSomeone/nestjs-service-template"
	},
	"author": "Alexey Filippov <socket.someone@gmail.com>",
	"contributors": [
		"Alexey Filippov <socket.someone@gmail.com>"
	],
	"dependencies": {
		"@nestjs/axios": "^4.0.0",
		"@nestjs/common": "11.1.3",
		"@nestjs/config": "^4.0.2",
		"@nestjs/core": "11.1.3",
		"@nestjs/jwt": "^11.0.0",
		"@nestjs/platform-express": "^11.1.3",
		"@nestjs/swagger": "^11.2.0",
		"axios": "^1.10.0",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.2",
		"reflect-metadata": "^0.2.2",
		"rxjs": "^7.8.2"
	},
	"devDependencies": {
		"@commitlint/cli": "19.8.1",
		"@commitlint/config-angular": "19.8.1",
		"@eslint/eslintrc": "^3.3.1",
		"@eslint/js": "^9.28.0",
		"@nestjs/cli": "^11.0.7",
		"@nestjs/schematics": "^11.0.5",
		"@nestjs/testing": "^11.1.3",
		"@release-it/conventional-changelog": "^10.0.1",
		"@swc/cli": "^0.7.7",
		"@swc/core": "^1.12.1",
		"@swc/jest": "^0.2.38",
		"@types/jest": "30.0.0",
		"@types/node": "24.0.3",
		"eslint": "^9.29.0",
		"eslint-config-prettier": "10.1.5",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-prettier": "5.5.0",
		"globals": "^16.2.0",
		"husky": "9.1.7",
		"jest": "30.0.0",
		"lint-staged": "16.1.2",
		"prettier": "3.5.3",
		"release-it": "^19.0.3",
		"rimraf": "6.0.1",
		"ts-jest": "29.4.0",
		"ts-loader": "^9.5.2",
		"ts-node": "10.9.2",
		"tsconfig-paths": "^4.2.0",
		"typescript": "5.8.3",
		"typescript-eslint": "^8.34.1"
	}
}
